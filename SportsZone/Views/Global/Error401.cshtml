@{
    ViewBag.Title = "Error401";
    Layout = "~/Views/Shared/_LayoutDashboard.cshtml";
}
<title>Access Denied!</title>
<div class="wrapper-page">

    <div class="container">
        <div class="row align-items-center justify-content-center">
            <div class="col-lg-5">
                <div class="card card-pages shadow-none mt-4">

                    <div class="text-center p-3">

                        <div class="img">
                            <img src="~/themes/ud/images/error.png" class="img-fluid" alt="Access Denied">
                        </div>

                        <h1 class="error-page mt-5"><span>401!</span></h1>
                        <h4 class="mb-4 mt-5">Access Denied</h4>
                        <p class="mb-4 w-75 mx-auto">You don't have permissions to navigate to that page!</p>
                        <a class="btn btn-primary mb-4 waves-effect waves-light" href="/account/me"><i class="mdi mdi-home"></i> Back to Dashboard</a>
                    </div>

                </div>

            </div>
        </div>
        <!-- end row -->
    </div>
</div>

